{
  "expo": {
    "name": "NRGsurf",
    "slug": "NRGsurf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_nrg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ufukyagmur.nrgsurf"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_nrg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ufukyagmur.nrgsurf"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_nrg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_nrg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86f58dd2-7887-440b-90a5-a6d7d8495f9b"
      }
    },
    "owner": "ufukygmr"
  }
}
